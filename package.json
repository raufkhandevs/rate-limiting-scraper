{
  "name": "rate-limiting-scraper",
  "version": "1.0.0",
  "description": "A Node.js Express TypeScript application for rate-limited web scraping",
  "main": "dist/setup.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/setup.ts",
    "build": "tsc",
    "start": "node dist/setup.js",
    "docker:build": "docker-compose build",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
